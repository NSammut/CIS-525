<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.umflint.multiactivity.ActivityC">

    <Button
        android:id="@+id/CtoA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To A"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="379dp"
        android:layout_below="@+id/activityCTextView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="33dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="69dp" />

    <Button
        android:id="@+id/CtoB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To B"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="379dp"
        android:layout_alignBaseline="@+id/CtoA"
        android:layout_alignBottom="@+id/CtoA"
        android:layout_toRightOf="@+id/activityCTextView"
        android:layout_toEndOf="@+id/activityCTextView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Activity C"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="95dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="98dp" />

    <TextView
        android:id="@+id/activityCTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nothing to see here..."
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="245dp"
        android:layout_marginTop="181dp"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
